{
	"name": "instantestimatecentral",
	"version": "1.0.0",
	"description": "",
	"homepage": "https://github.com/TehShrike/instantestimatecentral#readme",
	"bugs": {
		"url": "https://github.com/TehShrike/instantestimatecentral/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TehShrike/instantestimatecentral.git"
	},
	"imports": {
		"#lib/*": "./lib/*",
		"#pricing/*": "./pricing/*"
	},
	"license": "UNLICENSED",
	"author": "TehShrike",
	"type": "module",
	"main": "index.js",
	"packageManager": "pnpm@9.15.0",
	"scripts": {
		"regenerate_worker_types": "wrangler types ./worker/worker_types.d.ts",
		"test": "run-p test:*",
		"test:types:svelte": "svelte-check --no-tsconfig --fail-on-warnings",
		"test:types:worker": "tsc -p worker --noEmit",
		"test:types:everything_else": "tsc --noEmit",
		"test:types": "run-s -c test:types:*",
		"test:unit": "node --test embed/**/*.test.ts",
		"dev": "run-p dev:serve \"dev:watch -- {@} dev/build.js\" --",
		"dev:serve": "live-server --no-css-inject dev",
		"dev:watch": "node esbuild.config.ts --watch",
		"build": "run-p build:**",
		"build:worker": "node esbuild.worker.ts",
		"build:dufftreeservice:limb_removal": "node esbuild.config.ts -- dufftreeservice/limb_removal.svelte build/embed/dufftreeservice/limb_removal.js",
		"build:dufftreeservice:tree_removal": "node esbuild.config.ts -- dufftreeservice/tree_removal.svelte build/embed/dufftreeservice/tree_removal.js",
		"build:dufftreeservice:tree_trimming": "node esbuild.config.ts -- dufftreeservice/tree_trimming.svelte build/embed/dufftreeservice/tree_trimming.js",
		"build:dufftreeservice:tree_planting": "node esbuild.config.ts -- dufftreeservice/tree_planting.svelte build/embed/dufftreeservice/tree_planting.js",
		"build:dufftreeservice:all_estimates": "node esbuild.config.ts -- dufftreeservice/all_estimates.svelte build/embed/dufftreeservice/all_estimates.js",
		"build:www": "cp -r www build/",
		"postinstall": "mkdir -p build"
	},
	"devDependencies": {
		"@types/node": "^24.10.0",
		"esbuild": "^0.25.12",
		"esbuild-svelte": "^0.9.3",
		"live-server": "^1.2.2",
		"npm-run-all": "^4.1.5",
		"svelte": "^5.43.3",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"wrangler": "^4.46.0"
	},
	"dependencies": {
		"financial-number": "^4.0.4"
	}
}
