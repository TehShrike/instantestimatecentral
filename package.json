{
	"name": "instantestimatecentral",
	"version": "1.0.0",
	"description": "",
	"homepage": "https://github.com/TehShrike/instantestimatecentral#readme",
	"bugs": {
		"url": "https://github.com/TehShrike/instantestimatecentral/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TehShrike/instantestimatecentral.git"
	},
	"imports": {
		"#lib/*": "./lib/*",
		"#pricing/*": "./pricing/*"
	},
	"license": "UNLICENSED",
	"author": "TehShrike",
	"type": "module",
	"main": "index.js",
	"packageManager": "pnpm@9.15.0",
	"scripts": {
		"regenerate_worker_types": "wrangler types ./worker/worker_types.d.ts",
		"test": "run-p test:*",
		"test:types:svelte": "svelte-check --no-tsconfig --fail-on-warnings",
		"test:types:worker": "tsc -p worker --noEmit",
		"test:types:everything_else": "tsc --noEmit",
		"test:types": "run-s -c test:types:*",
		"test:unit": "node --test embed/**/*.test.ts",
		"dev": "run-p dev:serve dev:worker dev:watch_forms dev:reverse_proxy",
		"dev:serve": "live-server --no-css-inject dev",
		"dev:watch_forms": "node esbuild.forms.ts --watch",
		"dev:reverse_proxy": "caddy run",
		"dev:worker:build": "node esbuild.worker.ts --watch",
		"dev:worker:serve": "wrangler pages dev build --compatibility-date=2024-11-01 --port=8788",
		"dev:worker": "run-p dev:worker:*",
		"build": "run-p build:**",
		"build:forms": "node esbuild.forms.ts",
		"build:worker": "node esbuild.worker.ts",
		"build:www": "cp -r www build/",
		"postinstall": "mkdir -p build"
	},
	"devDependencies": {
		"@types/node": "^24.10.0",
		"esbuild": "^0.25.12",
		"esbuild-svelte": "^0.9.3",
		"live-server": "^1.2.2",
		"npm-run-all": "^4.1.5",
		"svelte": "^5.43.3",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"wrangler": "^4.48.0"
	},
	"dependencies": {
		"financial-number": "^4.0.4"
	}
}
